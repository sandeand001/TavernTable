    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0"
  },
  "lint-staged": {
    "{src,tests,tools}/**/*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.css": [
      "prettier --write",
      "stylelint -c tools/.stylelintrc.json --fix"
    ]
  },
  "keywords": [
    "game",
    "tabletop",
    "isometric",
    "pixi.js"
  ],
