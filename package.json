{
  "name": "taverntable",
  "version": "1.0.0",
  "description": "An isometric grid-based tabletop game interface",
  "main": "index.html",
  "scripts": {
    "format": "prettier --write \"{src,tests,tools,docs}/**/*.{js,css,html,json}\"",
    "format:check": "prettier --check \"{src,tests,tools,docs}/**/*.{js,css,html,json}\"",
    "test": "jest",
    "test:stable": "jest --detectOpenHandles --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint --cache \"{src,tools,tests}/**/*.js\" && stylelint -c tools/.stylelintrc.json \"src/**/*.css\"",
    "lint:js": "eslint --cache \"{src,tools,tests}/**/*.js\"",
    "lint:css": "stylelint -c tools/.stylelintrc.json \"src/**/*.css\"",
    "lint:fix": "eslint --cache \"{src,tools,tests}/**/*.js\" --fix && stylelint -c tools/.stylelintrc.json \"src/**/*.css\" --fix",
    "lint:layers": "node tools/check-layering.js",
    "lint:all": "npm run lint && npm run lint:layers",
    "serve": "python -m http.server 3000",
    "start": "npm run serve",
    "serve:open": "start \"\" http://localhost:3000 && npm run serve",
    "prepare": "husky",
    "map:update": "node ai/update_codebase_map.js"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "babel-jest": "^30.0.5",
    "canvas": "^3.1.2",
    "eslint": "^8.57.0",
